import urllib.request,urllib.parse,urllib.error 
file = urllib.request.urlopen('http://data.pr4e.org/romeo.txt')
dict = dict()
for lines in file :
                  words = lines.decode().split()
                  print(words)
                  for w in words :
                                    if w in dict :
                                                      dict[w] = dict[w] + 1
                                    else :
                                                      dict[w] = 1
                  print(dict)

lis = list()
for key,value in dict.items():
                  print(key,value)
                  new = value,key
                  lis.append(new)
                  
new = sorted(lis,reverse=True)
print(new)
